{
    "name": "devtemple/lara-lol",
    "description": "This is a package, which help you work simplier with League of Legends API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Kuriata",
            "email": "adriandj83@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Devtemple\\Laralol\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtemple\\Laralol\\LaralolServiceProvider"
            ],
            "aliases": {
                "Champion": "Devtemple\\Laralol\\Facades\\Champion",
                "Summoner": "Devtemple\\Laralol\\Facades\\Summoner",
                "LolStatus": "Devtemple\\Laralol\\Facades\\LolStatus",
            }
        }
    }
}
